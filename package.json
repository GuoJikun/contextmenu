{
  "name": "yak-contextmenu",
  "version": "1.0.1",
  "keywords": [
    "contextmenu",
    "vue3",
    "vue",
    "component",
    "yak-contextmenu"
  ],
  "files": [
    "dist/contextmenu.es.js",
    "dist/contextmenu.umd.js",
    "README.md"
  ],
  "main": "./dist/contextmenu.umd.js",
  "module": "./dist/contextmenu.es.js",
  "exports": {
    ".": {
      "import": "./dist/contextmenu.es.js",
      "require": "./dist/contextmenu.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lib": "vue-tsc --noEmit && vite build -c vite.config.lib.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GuoJikun/contextmenu.git"
  },
  "dependencies": {
    "vue": "^3.2.13"
  },
  "devDependencies": {
    "@types/node": "^16.9.6",
    "@vitejs/plugin-vue": "^1.9.0",
    "sass": "^1.42.0",
    "typescript": "^4.4.3",
    "vite": "^2.5.10",
    "vue-tsc": "^0.3.0"
  },
  "license": "MIT"
}
