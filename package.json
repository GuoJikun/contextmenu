{
  "name": "@jkun/contextmenu",
  "version": "1.0.0",
  "keywords": [
    "contextmenu",
    "vue3",
    "vue",
    "component",
    "mouseRightMenu",
    "rightMenu"
  ],
  "files": [
    "dist/contextmenu.mjs",
    "dist/contextmenu.umd.js",
    "dist/contextmenu.js",
    "dist/style.css",
    "README.md"
  ],
  "main": "dist/contextmenu.umd.js",
  "module": "dist/contextmenu.mjs",
  "browser": "dist/contextmenu.mjs",
  "exports": {
    ".": {
      "import": "./dist/contextmenu.es.js",
      "require": "./dist/contextmenu.umd.js",
      "browser": "./dist/contextmenu.mjs"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css",
      "browser": "./dist/style.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lib": "vue-tsc --noEmit && vite build -c vite.config.lib.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GuoJikun/contextmenu.git"
  },
  "peerDependencies": {
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@types/node": "^16.11.47",
    "@vitejs/plugin-vue": "^1.10.2",
    "sass": "^1.54.3",
    "typescript": "^4.7.4",
    "vite": "^3.0.4",
    "vue-tsc": "^0.3.0"
  },
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  }
}
